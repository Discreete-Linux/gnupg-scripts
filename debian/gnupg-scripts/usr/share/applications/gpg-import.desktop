[Desktop Entry]
Name=Import Key
Encoding=UTF-8
MimeType=application/pgp-keys;
Icon=gnupg-scripts-encrypt
Exec=/usr/bin/gpg-import
Type=Application
Terminal=false
NoDisplay=true
X-Ubuntu-Gettext-Domain=gnupg-scripts
